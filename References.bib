@article{duran_variabilite_2006,
	title = {Variabilité des modalités de débitage et des productions lithiques dans les industries moustériennes de la grotte de l’{Arbreda}, secteur alpha ({Serinyà}, {Espagne})},
	volume = {103},
	shorttitle = {Variabilité des modalités de débitage et des productions lithiques dans les industries moustériennes de la grotte de l’{Arbreda}, secteur alpha ({Serinyà}, {Espagne})},
	abstract = {L’Arbreda, secteur alpha, fouillé en 1972 par J.M. Corominas, présente une stratigraphie moustérienne de plus de 3 mètres (couches 30 à 44), comprise entre le début du stade isotopique 5 et le stade 3. Les sept ensembles stratigraphiques moustériens (G à A) se sont déposés durant des phases fortement humides et humides/fraîches (G à F), froides à extrêmement froides (E à C), puis plus tempéreés-fraîches (B à A). Les différents ensembles industriels de l’Arbreda peuvent être rattachés typologiquement au grand complexe du Moustérien typique méditerranéen de la frange orientale des Pyrénées, bien que fortement marqués par des influences charentiennes. L’abondance des collections (près de 14 000 pièces) et la variabilité des types de matières premières (porphyres, cornéennes, grès, calcaires, lydiennes et plusieurs types de quartz, quartzites et silex), toutes en quantités importantes, permettent différentes approches technologiques. Les patrimoines techniques hérités, les capacités d’adaptabilité et de compréhension des roches ainsi que leur mise en oeuvre, sont recherchés par la variabilité des méthodes et des techniques de production pour chaque type de roche à partir d’une approche diachronique. Selon une approche chronologique et toutes matières premières confondues sont décelées les évolutions générales des techniques et des modalités de production ainsi que leurs différentes associations intégrées par ces groupes moustériens sur une période de 80 000 ans. D’autant plus qu’une assez forte cohésion typologique se dessine tout au long de la stratigraphie. Cette continuité peut laisser entrevoir une certaine stabilité des groupes moustériens dans le sens de l’héritage et du patrimoine culturels. Ces études sont favorisées aussi par la continuité des grandes lignes technologiques de débitage qui s’articulent autour des méthodes discoïdes et Levallois. De ce fait, il est possible d’appréhender les évolutions et les associations technologiques importantes au travers de leurs différentes modalités.},
	number = {2},
	journal = {Bulletin de la Société Préhistorique Française},
	author = {Duran, Jean-Pierre and Soler, Narcís},
	year = {2006},
	keywords = {Lithic technology, Middle Paleolithic, Iberian Peninsula, Arbreda Cave},
	pages = {241--262},
	file = {Duran y Soler - 2006 - Variabilité des modalités de débitage et des produ.pdf:C\:\\Users\\Guillermo\\Zotero\\storage\\T9QFKWSS\\Duran y Soler - 2006 - Variabilité des modalités de débitage et des produ.pdf:application/pdf},
