@article{boeda_debitage_1993,
	title = {Le débitage discoïde et le débitage {Levallois} récurrent centripède},
	volume = {90},
	shorttitle = {Le débitage discoïde et le débitage {Levallois} récurrent centripède},
	doi = {10.3406/bspf.1993.9669},
	abstract = {Le débitage discoïde défini par F. Bordes a fait l'objet de nombreuses interprétations souvent divergentes. Parent pauvre de l'industrie lithique du Paléolithique par rapport au débitage Levallois, oublié dans les décomptes typologiques, tout au long de ces années le débitage discoïde avait perdu toute valeur de marqueur technique, se confondant avec d'autres modes de débitage. Or, l'analyse technologique a montré que le débitage discoïde correspondait à une réalité technique particulière, régi, comme tous les autres modes de débitage, par des règles techniques précises. Aussi se différencie-t-il du débitage Levallois et de la méthode Levallois récurrente centripète. Les confusions entre ces deux grands modes d'expression technique sont dues à l'absence d'une réelle lecture technique qui ne peut être que dynamique, prenant donc obligatoirement en compte l'ensemble des facteurs de variabilité inhérents au mode de débitage utilisé et résultant d'une action humaine et non pas mécanique.},
	number = {6},
	journal = {Bulletin de la Société Préhistorique Française},
	author = {Boëda, Eric},
	year = {1993},
	keywords = {Lithic technology, Discoid debitage, Levallois debitage},
	pages = {392--404},
	file = {Boëda - 1993 - Le débitage discoïde et le débitage Levallois récu.pdf:C\:\\Users\\Guillermo\\Zotero\\storage\\3T64DKMQ\\Boëda - 1993 - Le débitage discoïde et le débitage Levallois récu.pdf:application/pdf},
